{
  "name": "sacomi-sav",
  "description": "Web app for Sacomi's customer service.",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev:wss": "nodemon --watch src --ext .ts,.tsx,js,jsx --signal SIGTERM --exec \"ts-node --project tsconfig.server.json src/server/wssDevServer.ts\"",
    "dev:next": "tsc --project tsconfig.custom.json && node dist/set_env.js  && next dev",
    "dev": "run-p dev:*",
    "build": "run-s build:*",
    "build:next": "next build",
    "build:server": "tsc --project tsconfig.server.json",
    "start": "tsc --project tsconfig.custom.json && cross-env NODE_ENV=production WORK_ENV=local node dist/set_env.js && node dist/src/server/prodServer.js"
  },
  "dependencies": {
    "@headlessui/react": "1.6.6",
    "@heroicons/react": "1.0.6",
    "@prisma/client": "^4.6.1",
    "@tanstack/react-query": "^4.6.0",
    "@trpc/client": "^10.0.0-proxy-beta.7",
    "@trpc/next": "^10.0.0-proxy-beta.7",
    "@trpc/react": "^10.0.0-proxy-beta.7",
    "@trpc/server": "^10.0.0-proxy-beta.7",
    "axios": "^0.27.2",
    "bcrypt": "^5.0.1",
    "cross-env": "^7.0.3",
    "cross-var": "^1.1.0",
    "dotenv": "^16.0.2",
    "dotenv-cli": "^6.0.0",
    "multer": "^1.4.5-lts.1",
    "next": "12.3.1",
    "next-auth": "^4.12.0",
    "next-connect": "^0.13.0",
    "next-themes": "^0.2.1",
    "node-fetch": "2",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "superjson": "^1.10.0",
    "ts-node": "^10.9.1",
    "ws": "^8.11.0",
    "xml2js": "^0.4.23",
    "zod": "^3.19.1",
    "zustand": "^4.1.4"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/multer": "^1.4.7",
    "@types/node": "18.7.21",
    "@types/node-fetch": "^2.6.2",
    "@types/react": "18.0.21",
    "@types/react-dom": "18.0.6",
    "@types/ws": "^8.5.3",
    "@types/xml2js": "^0.4.11",
    "autoprefixer": "^10.4.12",
    "eslint": "8.24.0",
    "eslint-config-next": "12.3.1",
    "nodemon": "^2.0.20",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.16",
    "prisma": "^4.6.1",
    "tailwindcss": "^3.1.8",
    "typescript": "4.8.3"
  }
}
